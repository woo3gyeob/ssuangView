  "b2f2Kqt_KcE" : [ {
    "tStartMs": 5100,
    "dDurationMs": 2000,
    "segs": [ {
      "utf8": "who are you?"
    } ]
  }, {
    "tStartMs": 8500,
    "dDurationMs": 5060,
    "segs": [ {
      "utf8": "My name is Andy Sacks. I recently graduated from Northwestern University"
    } ]
  }, {
    "tStartMs": 13560,
    "dDurationMs": 1560,
    "segs": [ {
      "utf8": "And what are you doing here?"
    } ]
  }, {
    "tStartMs": 15460,
    "dDurationMs": 5740,
    "segs": [ {
      "utf8": "Well, I think I could do a good job as your assistant."
    } ]
  }, {
    "tStartMs": 21200,
    "dDurationMs": 3480,
    "segs": [ {
      "utf8": "And…"
    } ]
  }, {
    "tStartMs": 24680,
    "dDurationMs": 1980,
    "segs": [ {
      "utf8": "I came to New York to be a journalist"
    } ]
  }, {
    "tStartMs": 26660,
    "dDurationMs": 1900,
    "segs": [ {
      "utf8": "and sent letters out everywhere"
    } ]
  }, {
    "tStartMs": 28560,
    "dDurationMs": 4900,
    "segs": [ {
      "utf8": "and then finally got a call  from Elias Clarke and met with Sherry up at Human Resources."
    } ]
  }, {
    "tStartMs": 33460,
    "dDurationMs": 1220,
    "segs": [ {
      "utf8": "and..."
    } ]
  }, {
    "tStartMs": 34860,
    "dDurationMs": 2300,
    "segs": [ {
      "utf8": "Basically it´s this or Auto Universe."
    } ]
  }, {
    "tStartMs": 37160,
    "dDurationMs": 2400,
    "segs": [ {
      "utf8": "So, you don´t read Runway?"
    } ]
  }, {
    "tStartMs": 39560,
    "dDurationMs": 1980,
    "segs": [ {
      "utf8": "Oh, no!"
    } ]
  }, {
    "tStartMs": 41540,
    "dDurationMs": 4100,
    "segs": [ {
      "utf8": "And before today, you had never heard of me?"
    } ]
  }, {
    "tStartMs": 46560,
    "dDurationMs": 720,
    "segs": [ {
      "utf8": "No"
    } ]
  }, {
    "tStartMs": 47320,
    "dDurationMs": 3540,
    "segs": [ {
      "utf8": "And You have no style or sense of fashion."
    } ]
  }, {
    "tStartMs": 51380,
    "dDurationMs": 4000,
    "segs": [ {
      "utf8": "Well, I think that depends on what you´re .."
    } ]
  }, {
    "tStartMs": 55380,
    "dDurationMs": 3380,
    "segs": [ {
      "utf8": "No, no, that wasn´t a question."
    } ]
  }, {
    "tStartMs": 62000,
    "dDurationMs": 2780,
    "segs": [ {
      "utf8": "Um, I was editor in chief of the Daily Northwestern."
    } ]
  }, {
    "tStartMs": 64780,
    "dDurationMs": 4700,
    "segs": [ {
      "utf8": "I also won a national competition for college journalist"
    } ]
  }, {
    "tStartMs": 69480,
    "dDurationMs": 2900,
    "segs": [ {
      "utf8": "with my series on the janitors union which exposed the exploitation of the..."
    } ]
  }, {
    "tStartMs": 72380,
    "dDurationMs": 1400,
    "segs": [ {
      "utf8": "That´s all..."
    } ]
  }, {
    "tStartMs": 82420,
    "dDurationMs": 2000,
    "segs": [ {
      "utf8": "Yeah, yeah, Ok, You´re right."
    } ]
  }, {
    "tStartMs": 84420,
    "dDurationMs": 1040,
    "segs": [ {
      "utf8": "I don´t fit in here."
    } ]
  }, {
    "tStartMs": 85620,
    "dDurationMs": 5860,
    "segs": [ {
      "utf8": "I´m not skinny or glamorous and I don´t know that much about fashion"
    } ]
  }, {
    "tStartMs": 91480,
    "dDurationMs": 5920,
    "segs": [ {
      "utf8": "but I´m smart. I learn fast and I will work very hard…."
    } ]
  }, {
    "tStartMs": 105460,
    "dDurationMs": 2000,
    "segs": [ {
      "utf8": "thank you for your time!"
    } ]
  }, {
    "tStartMs": 107460,
    "dDurationMs": 2000,
    "segs": [ {
      "utf8": "Who is that sad little person?"
    } ]
  } ],
  "uKQknI9EdrM": [ {
    "tStartMs": 70,
    "dDurationMs": 2730,
    "segs": [ {
      "utf8": "Hi! I'm Tim from BBC Learning English,"
    } ]
  }, {
    "tStartMs": 2800,
    "dDurationMs": 2520,
    "segs": [ {
      "utf8": "here to tell you about two words we use to say"
    } ]
  }, {
    "tStartMs": 5320,
    "dDurationMs": 1640,
    "segs": [ {
      "utf8": "that things are similar."
    } ]
  }, {
    "tStartMs": 6960,
    "dDurationMs": 2420,
    "segs": [ {
      "utf8": "We use 'like' as a preposition"
    } ]
  }, {
    "tStartMs": 9380,
    "dDurationMs": 3880,
    "segs": [ {
      "utf8": "before a noun or pronoun, and it means 'similar to'."
    } ]
  }, {
    "tStartMs": 13260,
    "dDurationMs": 2220,
    "segs": [ {
      "utf8": "He ran like the wind."
    } ]
  }, {
    "tStartMs": 15480,
    "dDurationMs": 2500,
    "segs": [ {
      "utf8": "Not: He ran as the wind."
    } ]
  }, {
    "tStartMs": 17980,
    "dDurationMs": 4240,
    "segs": [ {
      "utf8": "We use 'like', and not 'as', to compare appearances."
    } ]
  }, {
    "tStartMs": 22220,
    "dDurationMs": 2720,
    "segs": [ {
      "utf8": "This house looks like a castle."
    } ]
  }, {
    "tStartMs": 24940,
    "dDurationMs": 2560,
    "segs": [ {
      "utf8": "'As' can be used as a preposition."
    } ]
  }, {
    "tStartMs": 27500,
    "dDurationMs": 4240,
    "segs": [ {
      "utf8": "It means 'in the role of'."
    } ]
  }, {
    "tStartMs": 31740,
    "dDurationMs": 700,
    "segs": [ {
      "utf8": "Dan,"
    } ]
  }, {
    "tStartMs": 32440,
    "dDurationMs": 3660,
    "segs": [ {
      "utf8": "as your friend I have to say you're not a good singer."
    } ]
  }, {
    "tStartMs": 36100,
    "dDurationMs": 3620,
    "segs": [ {
      "utf8": "We often use 'as' to talk about people's jobs."
    } ]
  }, {
    "tStartMs": 39720,
    "dDurationMs": 2140,
    "segs": [ {
      "utf8": "I work as an actor."
    } ]
  }, {
    "tStartMs": 41860,
    "dDurationMs": 2420,
    "segs": [ {
      "utf8": "Be careful using 'like' and 'as'"
    } ]
  }, {
    "tStartMs": 44280,
    "dDurationMs": 2000,
    "segs": [ {
      "utf8": "because the meaning can change:"
    } ]
  }, {
    "tStartMs": 46280,
    "dDurationMs": 2580,
    "segs": [ {
      "utf8": "'As your brother, I'll try to help you'"
    } ]
  }, {
    "tStartMs": 48860,
    "dDurationMs": 2480,
    "segs": [ {
      "utf8": "means 'I actually am your brother'."
    } ]
  }, {
    "tStartMs": 51340,
    "dDurationMs": 2400,
    "segs": [ {
      "utf8": "Change it to 'like', and it means"
    } ]
  }, {
    "tStartMs": 53740,
    "dDurationMs": 3400,
    "segs": [ {
      "utf8": "'I'm not your brother but I want to act in a similar way'."
    } ]
  } ]
}