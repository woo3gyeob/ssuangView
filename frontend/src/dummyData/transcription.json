  [ {
    "tStartMs": 0,
    "dDurationMs": 7000,
    "segs": [ {
      "utf8": "Transcriber: Victor Borges\nReviewer: David DeRuwe"
    } ]
  }, {
    "tStartMs": 11791,
    "dDurationMs": 1008,
    "segs": [ {
      "utf8": "Wow,"
    } ]
  }, {
    "tStartMs": 13030,
    "dDurationMs": 1190,
    "segs": [ {
      "utf8": "what an audience."
    } ]
  }, {
    "tStartMs": 14515,
    "dDurationMs": 3016,
    "segs": [ {
      "utf8": "But if I'm being honest,\nI don't care what you think of my talk."
    } ]
  }, {
    "tStartMs": 18097,
    "dDurationMs": 1004,
    "segs": [ {
      "utf8": "I don't."
    } ]
  }, {
    "tStartMs": 19101,
    "dDurationMs": 1999,
    "segs": [ {
      "utf8": "I care what the internet\nthinks of my talk."
    } ]
  }, {
    "tStartMs": 21100,
    "dDurationMs": 1001,
    "segs": [ {
      "utf8": "(Laughter)"
    } ]
  }, {
    "tStartMs": 22101,
    "dDurationMs": 2478,
    "segs": [ {
      "utf8": "Because they are the ones\nwho get it seen and shared."
    } ]
  }, {
    "tStartMs": 24579,
    "dDurationMs": 2328,
    "segs": [ {
      "utf8": "And I think that's where\nmost people get it wrong."
    } ]
  }, {
    "tStartMs": 26907,
    "dDurationMs": 1667,
    "segs": [ {
      "utf8": "They're talking to you, here,"
    } ]
  }, {
    "tStartMs": 28660,
    "dDurationMs": 4630,
    "segs": [ {
      "utf8": "instead of talking to you,\nrandom person scrolling Facebook."
    } ]
  }, {
    "tStartMs": 34323,
    "dDurationMs": 1180,
    "segs": [ {
      "utf8": "Thanks for the click."
    } ]
  }, {
    "tStartMs": 36141,
    "dDurationMs": 1684,
    "segs": [ {
      "utf8": "You see, back in 2009,"
    } ]
  }, {
    "tStartMs": 37855,
    "dDurationMs": 3545,
    "segs": [ {
      "utf8": "we all had these weird little things\ncalled attention spans."
    } ]
  }, {
    "tStartMs": 41400,
    "dDurationMs": 1000,
    "segs": [ {
      "utf8": "(Laughter)"
    } ]
  }, {
    "tStartMs": 42400,
    "dDurationMs": 3196,
    "segs": [ {
      "utf8": "Yeah, they're gone. They're gone.\nWe killed them. They're dead."
    } ]
  }, {
    "tStartMs": 46287,
    "dDurationMs": 3773,
    "segs": [ {
      "utf8": "I'm trying to think of the last time\nI watched an 18-minute TED talk."
    } ]
  }, {
    "tStartMs": 50140,
    "dDurationMs": 2500,
    "segs": [ {
      "utf8": "It's been years, literally years."
    } ]
  }, {
    "tStartMs": 52865,
    "dDurationMs": 2695,
    "segs": [ {
      "utf8": "So if you're giving\na TED talk, keep it quick."
    } ]
  }, {
    "tStartMs": 55700,
    "dDurationMs": 1800,
    "segs": [ {
      "utf8": "I'm doing mine in under a minute."
    } ]
  }, {
    "tStartMs": 57560,
    "dDurationMs": 1595,
    "segs": [ {
      "utf8": "I'm at 44 seconds right now;"
    } ]
  }, {
    "tStartMs": 59155,
    "dDurationMs": 2128,
    "segs": [ {
      "utf8": "that means we've got\ntime for one final joke."
    } ]
  }, {
    "tStartMs": 61450,
    "dDurationMs": 3050,
    "segs": [ {
      "utf8": "Why are balloons so expensive?"
    } ]
  }, {
    "tStartMs": 64580,
    "dDurationMs": 1125,
    "segs": [ {
      "utf8": "(Audience) \"Why?\""
    } ]
  }, {
    "tStartMs": 65705,
    "dDurationMs": 1236,
    "segs": [ {
      "utf8": "Woody Roseland: Inflation."
